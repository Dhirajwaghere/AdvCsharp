  <script>

  	var name="Dhiraj";

  document.write("<br>" + name);


  	var age = 25;
        for (let i = 1; i <= 10; i++) {
            document.write("<br>"+i)
        }


        if (age > 18) {
            document.write("<br>"+"Your age is valid")
        }
        else {
            document.write("Your age is not valid")
        }


    var   num=50;
         sum=0;
    while(num>0)
            {
                let r = num % 10;
                sum = sum + r;
                num = num / 10;

            }
     	document.write("<br>" + sum);


     	let a=10;
     	do{
     	document.write("<br>"+a);
     	}
     	while(a>=50);

</script>
